{
  "extends": "@tsconfig/node24/tsconfig.json",
  "include": [
    "vite.config.*",
    "vitest.config.*",
    "cypress.config.*",
    "nightwatch.conf.*",
    "playwright.config.*",
    "eslint.config.*"
  ],
"compilerOptions": {
  "target": "ESNext",
  "module": "ESNext",
  "moduleResolution": "Node",
  "strict": true,
  "jsx": "preserve",
  "esModuleInterop": true,
  "skipLibCheck": true,
  "types": ["vite/client", "node"]
}
}
